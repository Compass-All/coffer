echo 0 > /proc/sys/kernel/hung_task_panic
echo 0 > /proc/sys/kernel/softlockup_panic
echo 0 > /proc/sys/kernel/watchdog
dmesg -n 1
taskset 0x1 /bin/bash


repeat=42
start=2
for i in `seq $start $(($repeat + $start - 1))`
do
	taskset 0xe /prog/host/create_enclave && taskset 0xe /prog/host/enter_enclave $i /prog/enclave/rv8/rv8_qsort
done


for i in `seq $start $(($repeat + $start - 1))`
do
	taskset 0xe /prog/host/test_concurrent $i 0 &
done
